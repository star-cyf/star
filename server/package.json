{
  "name": "star-server",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "star-server",
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc",
    "dev": "ts-node-dev src/index.ts",
    "prettier": "node node_modules/prettier/bin/prettier.cjs --config .prettierrc . --write",
    "lint:fix": "node node_modules/eslint/bin/eslint.js . --fix",
    "test": "cross-env NODE_ENV=test node node_modules/jest/bin/jest.js --runInBand",
    "database:migration:check": "drizzle-kit check:pg --config=./src/database/config.ts",
    "database:update:metadata": "drizzle-kit up:pg --config=./src/database/config.ts",
    "database:migration:generate": "drizzle-kit generate:pg --config=./src/database/config.ts",
    "database:migration:apply": "ts-node src/database/applyMigration.ts",
    "database:migration": "npm run database:migration:check && npm run database:update:metadata && npm run database:migration:generate && npm run database:migration:apply && npm run database:seed",
    "database:seed": "ts-node src/database/seed.ts",
    "database:studio": "drizzle-kit studio --config=./src/database/config.ts"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-flow": "^4.0.1",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "google-auth-library": "^9.2.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.2",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
